<p id="notice"><%= notice %></p>

<h1>Service/Beneficiaires</h1>
<div >
<%=form_tag(service_beneficiaires_path, method: :get) do %>
<%= date_field_tag :term, params[:term] %>
<%=submit_tag :Search  %>
<% end %>
</div>
<table>
  <thead>
    <tr>
      <th>Cout</th>
      <th>Date service</th>
      <th>Service</th>
      <th>Beneficiaire</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_beneficiaires.each do |service_beneficiaire| %>
      <tr  class="<%=cycle('list_line_even','list_line_odd') %>">
        <td><%= service_beneficiaire.cout %></td>
        <td><%= service_beneficiaire.date_service %></td>
        <td><%= service_beneficiaire.service.nom %></td>
        <td><%= service_beneficiaire.beneficiaire.nom %></td>
        <td><%= link_to 'Show', service_beneficiaire %></td>
        <td><%= link_to 'Edit', edit_service_beneficiaire_path(service_beneficiaire) %></td>
        <td><%= link_to 'Destroy', service_beneficiaire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="btn btn-default">
<%= link_to 'Nouveau Service_Beneficiaire', new_service_beneficiaire_path %>
</div>
