</div>
<div class="field">
  <%= f.label :age %>
  <%= f.number_field :age %>
</div>
<div class="field">
  <%= f.label :sexe %>
  <%= f.text_field :sexe %>
</div>
<div class="field">
  <%= f.label :etat_medical %>
  <%= f.text_field :etat_medical %>
</div>
<div class="field">
  <%= f.label :remarque %>
  <%= f.text_area :remarque %>
</div>
<div class="button">

  <%= f.submit %>
</div>

<% end %>





