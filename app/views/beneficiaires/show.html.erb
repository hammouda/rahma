<h2> Ajouter un membre famille</h2>
<%= form_for ([@beneficiaire, @beneficiaire.members.build]) do |f| %>
<div class="field">
  <%= f.label :nom %>
  <%= f.text_field :nom %>
</div>
<div class="field">
  <%= f.label :prenom %>
  <%= f.text_field :prenom %>
</div>
<div class="field">
  <%= f.label :date_naissance %>
